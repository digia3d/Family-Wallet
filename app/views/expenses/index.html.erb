<%= render partial: "layouts/navbar", locals: {heading: 'EXPENSES', back_path: categories_path} %>
<div class="container">
<p style="color: green"><%= notice %></p>

<div id="expenses">
  <% @expenses.each do |expense| %>
    <%= render expense %>
    <p>
      <%= link_to "Show this expense", category_expense_path(expense.category_id, expense) %>
    </p>
  <% end %>
</div>
</div>
